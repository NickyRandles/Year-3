
public class Main {
	
	public static void main(String [] args)
	{
	

		Stack stack = new Stack();

		
		char [] braces = {'{', '{', '}', '}'};
		
		stack.display();
		
		boolean balancedSoFar = true;
		int k = 0;
		int input = stack.input;
		
		
		while(balancedSoFar == true && k <= input)
		{
			if(braces[0] == '{')
			{
				stack.push('{');
			}		stack.display();

		}
		

	
	}

}
