
public class Link {
	
	public char bracket;
	
	public Link next;
	
	public Link(char bracket)
	{
		this.bracket = bracket;
	}
	
	public void display()
	{
		System.out.println(bracket);
	}
	
	public char toChar()
	{
		return bracket;
	}

	public static void main(String [] args)
	{
		Stack stack = new Stack();
		
		stack.push('h');
		stack.push('j');
		stack.popAll();
		
		stack.display();
	
	}
	

}
