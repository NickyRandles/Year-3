
public class Main {
	
	public static void main(String [] args)
	{
	

		Stack stack = new Stack();

		
		String [] braces = {"{", "{", "}", "}"};
		
		stack.display();
		
		boolean balancedSoFar = true;
		int k = 0;
		int input = stack.input;
		char bracket = (Character) null;
		
		while(balancedSoFar == true && k <= input)
		{
			if(bracket == '{')
			{
				stack.push(;)
			}
		}
		
		stack.display();

	
	}

}
